{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "vite/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowSyntheticDefaultImports":true,
    "paths": {
      "@/*": [
        "*"
      ]
    }
  }
}